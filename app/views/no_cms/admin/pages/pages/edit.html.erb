<% content_for :second_menu do %>
  <%= render partial: 'page_listing_item', collection: @roots, as: 'page', layout: 'page_listing' unless @roots.blank? %>
<% end %>

<%= form_for @page do |f| %>
  <%= render 'content_fields', f: f %>
  <%= render 'no_cms/admin/pages/blocks/index', f: f %>

  <%= f.submit t('.submit') %>
<% end %>
